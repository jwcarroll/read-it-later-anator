{
  "name": "read-it-later-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Read-It-Later-Anator",
  "bin": {
    "infrastructure": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all --require-approval never",
    "deploy:dev": "cdk deploy --all --require-approval never -c environment=dev",
    "deploy:prod": "cdk deploy --all --require-approval never -c environment=prod",
    "diff": "cdk diff",
    "destroy": "cdk destroy --all"
  },
  "keywords": [
    "aws-cdk",
    "infrastructure",
    "dynamodb",
    "s3",
    "sqs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.110.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
